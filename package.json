{
  "name": "cinema-manager",
  "version": "1.0.0",
  "description": "This is an application that works as the core cinema manager",
  "main": "server.ts",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node17": "^1.0.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "NODE_PATH=$NODE_PATH:./src nodemon --exec babel-node src/index.ts",
    "tsc": "tsc --traceResolution",
    "dev": "NODE_PATH=$NODE_PATH:./src ts-node-dev --respawn --transpile-only src/server.ts",
    "develop": "NODE_ENV=development nodemon ./src/app.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.1",
    "prisma-client": "^0.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "uuid": "^9.0.0"
  }
}
